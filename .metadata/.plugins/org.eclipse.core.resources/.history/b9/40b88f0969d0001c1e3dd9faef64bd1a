/**
 * 
 */
 
 $(document).ready(function() 
{ 
if ($("#alertSuccess").text().trim() == "") { 
 $("#alertSuccess").hide(); 
 } 
 $("#alertError").hide(); 
});


$(document).on("click", "#btnSave", function(event){ 
 $("#alertSuccess").text(""); 
 $("#alertSuccess").hide(); 
 $("#alertError").text(""); 
 $("#alertError").hide();
});